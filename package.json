    "version": "v3.2.0",
    "author": {
        "name": "Sifatul Rabbi",
        "email": "mdsiftulislam.rabbi@gmail.com",
        "url": "https://sifatulrabbi.github.io"
    },
    "keywords": [
        "next.js",
        "react",
        "typescript",
        "tailwind",
        "node.js",
        "express.js"
    ],
    "engines": {
        "node": "18"
    },
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start-next": "NODE_ENV=production next start",
        "start": "NODE_ENV=production node ./server.js",
        "export": "NODE_ENV=production next build && next export",
        "lint": "next lint",
        "predeploy": "npm run build && npm run export && touch ./out/.nojekyll && echo 'www.sifatulrabbi.github.io' > ./out/CNAME",
        "deploy": "gh-pages -d ./out -t true",
        "format": "prettier --write ./"
    },
    "dependencies": {
        "dayjs": "^1.11.9",
        "next": "^12.1.4",
        "react": "^18.0.0-rc.3",
        "react-dom": "^18.0.0-rc.3",
        "react-icons": "^4.11.0",
        "react-in-viewport": "^1.0.0-alpha.26",
        "recoil": "^0.7.0",
        "sharp": "^0.31.3",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/node": "17.0.22",
        "@types/react": "17.0.42",
        "@types/uuid": "^8.3.4",
        "autoprefixer": "^10.4.4",
        "eslint": "8.11.0",
        "eslint-config-next": "12.1.0",
        "gh-pages": "^3.2.3",
        "postcss": "^8.4.12",
        "prettier": "^2.6.0",
        "sass": "^1.54.5",
        "tailwindcss": "^3.0.23",
        "typescript": "4.6.2"
    }
}
