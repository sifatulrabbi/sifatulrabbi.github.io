{
  "editor.fontFamily": "'Fira Code Retina', 'jetBrains Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.formatOnSave": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.renderWhitespace": "none",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.cursorStyle": "line",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "tabnine.experimentalAutoImports": true,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.fontFamily": "'Fira Code Retina', 'jetBrains Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.inlineHints.fontFamily": "'jetBrains mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.codeLensFontFamily": "'Fira Code Retina', 'jetBrains Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "rest-client.fontFamily": "",
  "editor.fontLigatures": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.hideCursorInOverviewRuler": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.gpuAcceleration": "on",
  "editor.lineHeight": 22,
  "editor.suggestLineHeight": 20,
  "explorer.confirmDragAndDrop": false,
  "editor.inlayHints.fontFamily": "'Fira Code Retina', 'jetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "expand",
  "diffEditor.codeLens": true,
  "editor.inlineSuggest.enabled": true,
  "files.maxMemoryForLargeFilesMB": 6144,
  "workbench.list.smoothScrolling": true,
  "workbench.editor.closeOnFileDelete": true,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.enableBell": true,
  "explorer.confirmDelete": false,
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.editor.tabCloseButton": "left",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "python.pythonPath": "/usr/bin/python3",
  "javascript.format.semicolons": "insert",
  "typescript.format.semicolons": "insert",
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "~/CSS/"
    }
  ],
  "html.format.wrapLineLength": 100,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.language": "en,en-US",
  "editor.minimap.size": "fit",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "prettier.singleQuote": true,
  "diffEditor.wordWrap": "off",
  "liveSassCompile.settings.autoprefix": [],
  "prettier.printWidth": 90,
  "terminal.integrated.tabs.location": "left",
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "security.workspace.trust.untrustedFiles": "open",
  "scm.inputFontSize": 14,
  "scm.inputFontFamily": "'Fira Code Retina', default",
  "debug.console.fontFamily": "'Fira Code Retina', default",
  "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Roboto', 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "prettier.tabWidth": 4,
  "terminal.integrated.tabs.enabled": true,
  "prettier.jsxSingleQuote": true,
  "workbench.colorTheme": "Night Wolf [dark blue]",
  "cSpell.words": ["nobg"]
}
